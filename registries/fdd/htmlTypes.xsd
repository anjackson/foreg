<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified" attributeFormDefault="unqualified">
<!--  For inclusion in fdd.xsd, schema for Format Description Documents, to handle HTML styling within FDDs -->
<!--  VERSION 1.0, July 20, 2012-->
<!--     Declarations modified July 20, 2012 to support public access to FDD schema -->
	<xsd:complexType name="notesText" mixed="true">
		<xsd:sequence minOccurs="0" maxOccurs="unbounded">
			<xsd:element name="b" type="xsd:string" minOccurs="0"/>
			<xsd:element name="i" type="xsd:string" minOccurs="0"/>
			<xsd:element name="sup" type="xsd:string" minOccurs="0"/>
			<xsd:element name="p" type="paragraphType" minOccurs="0"/>
			<xsd:element name="br" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="a" minOccurs="0">
				<xsd:complexType mixed="true">
					<xsd:attribute name="href" type="xsd:string" use="required"/>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ul" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence maxOccurs="unbounded">
						<xsd:element name="li" type="notesText" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="fddLink" minOccurs="0">
				<xsd:complexType mixed="true">
					<xsd:attribute name="id" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:pattern value="fdd[0-9]{6}"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="footNote" minOccurs="0">
				<xsd:complexType>
					<xsd:attribute name="id" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:pattern value="[1-9]{1}[0-9]*"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
